#extract all these to EXTRACT CHECK
exe=/u/modiaz/UPPER_DIVISION/Fall25/CS333/CS333/labs/labs/lab2/arvik-md4
his=/u/modiaz/UPPER_DIVISION/Fall25/CS333/CS333/labs/labs/lab2/3.arvik
mine=/u/modiaz/UPPER_DIVISION/Fall25/CS333/CS333/labs/labs/lab2/chaney0.arvik
saint=/u/modiaz/UPPER_DIVISION/Fall25/CS333/CS333/labs/labs/lab2
white_devil=$saint


ccurrent_action='-c'
cargs="$ccurrent_action -f"
cvargs="$ccurrent_action -v -f"
cVargs="$ccurrent_action -V -f"
cEVILargs="$ccurrent_action '-v -V -f"

echo "into his c"
$exe $cargs $saint/CRTS/simple3.arvik $white_devil/1-s.txt $white_devil/2-s.txt $white_devil/3-s.txt
$exe $cvargs $saint/CRTS/verbose3.arvik $white_devil/1-s.txt $white_devil/2-s.txt $white_devil/3-s.txt
$exe $cVargs $saint/CRTS/verify3.arvik $white_devil/1-s.txt $white_devil/2-s.txt $white_devil/3-s.txt
$exe $cEVILargs $saint/CRTS/EVIL3.arvik $white_devil/1-s.txt $white_devil/2-s.txt $white_devil/3-s.txt

echo "into my c"
$exe $cargs $saint/CRTS/simple0.arvik $white_devil/0-s.txt
$exe $cvargs $saint/CRTS/verbose0.arvik $white_devil/0-s.txt 
$exe $cVargs $saint/CRTS/verify0.arvik $white_devil/0-s.txt 
$exe $cEVILargs $saint/CRTS/EVIL0.arvik $white_devil/0-s.txt

current_action='-x'
args="$current_action -f"
vargs="$current_action -v -f"
Vargs="$current_action -V -f"
EVILargs="$current_action '-v -V -f"


echo "into his x"
echo "1"
$exe $args $his > $saint/EXTS/simple3.txt
echo "2"
$exe $vargs $his > $saint/EXTS/verbose3.txt
echo "3"
$exe $Vargs $his > $saint/EXTS/verify3.txt
echo "4"
$exe $EVILargs $his > $saint/EXTS/EVIL3.txt

echo "into my x"
$exe $args $mine > $saint/EXTS/simple0.txt
$exe $vargs $mine > $saint/EXTS/verbose0.txt
$exe $Vargs $mine > $saint/EXTS/verify0.txt
$exe $EVILargs $mine > $saint/EXTS/EVIL0.txt

